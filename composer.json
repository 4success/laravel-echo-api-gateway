{
    "name": "aliasproject/laravel-echo-api-gateway",
    "description": "Laravel Echo with API Gateway Websockets",
    "keywords": [
        "laravel",
        "echo",
        "websockets"
    ],
    "homepage": "https://github.com/aliasproject/laravel-echo-api-gateway",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Aguiar",
            "email": "mike@aliasproject.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.294",
        "bref/bref": "^2.1",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "nunomaduro/larastan": "^0.7",
        "orchestra/testbench": "^6.9",
        "pestphp/pest": "^1.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AliasProject\\LaravelEchoApiGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliasProject\\LaravelEchoApiGateway\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.12"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
